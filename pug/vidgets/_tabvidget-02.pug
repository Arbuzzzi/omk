// TABVIDGET
#tabvidget2.vidget-box.vidget-tabbox
	nav
		#nav-tab.nav.nav-tabs.vidget-items(role="tablist")
			a#nav-contact-tab.nav-item.vidget-item(data-toggle="tab" href="#vidget2-tab3" role="tab" aria-controls="vidget2-tab3" aria-selected="false") Доска почета
			a#nav-profile-tab.nav-item.vidget-item(data-toggle="tab" href="#vidget2-tab2" role="tab" aria-controls="vidget2-tab2" aria-selected="false") День рождения
			a#nav-home-tab.nav-item.vidget-item.active(data-toggle="tab" href="#vidget2-tab1" role="tab" aria-controls="vidget2-tab1" aria-selected="true") Назначение
	#nav-tabContent.tab-content.vidget-tabs
		#vidget2-tab3.tab-pane.vidget-tab.fade(role="tabpanel" aria-labelledby="nav-contact-tab")
			.vidget-content
				.vidget-content__left
					.vidget-head
						.vidget-title
							| ДОСКА 
							br
							| ПОЧЕТА
					.vidget-footer
						.vidget-text
							p
								strong
									| Контаревский 
									br
									| Николай 
									br
									| Константинович 
									br
								| Председатель 
								br
								| подразделения 28 
								br
				.vidget-content__right
					.vidget-imgbox
						img(src="img/vidget-img-07.jpg" alt="image")
		#vidget2-tab2.tab-pane.vidget-tab.fade(role="tabpanel" aria-labelledby="nav-profile-tab")
			.vidget-content
				.vidget-content__left
					.vidget-head
						.vidget-title
							| ДЕНЬ 
							br
							| РОЖДЕНИЯ
					.vidget-footer
						.vidget-text
							p
								strong
									| Тушин 
									br
									| Петр 
									br
									| Николаевич 
									br
								| Директор по 
								br
								| управлению 
								br
								| эффективностью
				.vidget-content__right
					.vidget-imgbox
						img(src="img/vidget-img-05.png" alt="image")
		#vidget2-tab1.tab-pane.vidget-tab.fade.show.active(role="tabpanel" aria-labelledby="nav-home-tab")
			.vidget-content
				.vidget-content__left
					.vidget-head
						.vidget-title
							| НАЗНАЧЕНИЕ
					.vidget-footer
						.vidget-text
							p
								strong
									| Еремина 
									br
									| Наталья 
									br
									| Константиновна 
									br
								| Председатель 
								br
								| правления ОМК
				.vidget-content__right
					.vidget-imgbox
						img(src="img/vidget-img-06.jpg" alt="image")