// TABVIDGET
#tabvidget1.vidget-box.vidget-tabbox
	nav
		#nav-tab.nav.nav-tabs.vidget-items(role="tablist")
			a#nav-home-tab.nav-item.vidget-item(data-toggle="tab" href="#vidget1-tab1" role="tab" aria-controls="vidget1-tab1" aria-selected="false") Годовой отчет
			a#nav-profile-tab.nav-item.vidget-item(data-toggle="tab" href="#vidget1-tab2" role="tab" aria-controls="vidget1-tab2" aria-selected="false") Территория ОМК
			a#nav-contact-tab.nav-item.vidget-item.active(data-toggle="tab" href="#vidget1-tab3" role="tab" aria-controls="vidget1-tab3" aria-selected="true") ОМК-команда
	#nav-tabContent.tab-content.vidget-tabs
		#vidget1-tab1.tab-pane.vidget-tab.fade.show.active(role="tabpanel" aria-labelledby="nav-contact-tab")
			.vidget-content
				.vidget-content__left
					.vidget-head
						.vidget-title
							| ГОДОВОЙ 
							br
							| ОТЧЕТ ОМК
					.vidget-footer
						a.vidget-link(href="#")
							| Web-версия 
							br
							| Скачать PDF
				.vidget-content__right
					.vidget-imgbox
						img(src="img/vidget-img-01.jpg" alt="image")
		#vidget1-tab2.tab-pane.vidget-tab.fade(role="tabpanel" aria-labelledby="nav-profile-tab")
			.vidget-content
				.vidget-content__left
					.vidget-head
						.vidget-title
							| ТЕРРИТОРИЯ 
							br
							| ОМК
					.vidget-footer
						a.vidget-link(href="#")
							| 16.11.2018 
							br
							| №43 (154) 2018
				.vidget-content__right
					.vidget-imgbox
						img(src="img/vidget-img-02.jpg" alt="image")
		#vidget1-tab3.tab-pane.vidget-tab.fade(role="tabpanel" aria-labelledby="nav-home-tab")
			.vidget-content
				.vidget-content__left
					.vidget-head
						.vidget-title
							| КОРПОРАТИВНАЯ 
							br
							| ПРЕССА
					.vidget-footer
						a.vidget-link(href="#")
							| Журнал 
							br
							| ОМК-команда 
							br
							| выпуск 34, 2018
				.vidget-content__right
					.vidget-imgbox
						img(src="img/vidget-img-04.jpg" alt="image")